<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to pat(:list), url(:professional_constructions, :index) %></li>
      <li><%= link_to pat(:new), url(:professional_constructions, :new) %></li>
      <li class="active"><%= link_to pat(:edit), url(:professional_constructions, :edit, :id => @professional_construction.id) %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= pat(:edit) %> <% mt(:professional_construction) %></h2>
    <div class="inner">
      <% form_for :professional_construction, url(:professional_constructions, :update, :id => @professional_construction.id), :method => :put, :class => :form do |f| %>
        <%= partial "professional_constructions/form", :locals => { :f => f } %>
      <% end %>
    </div>
  </div>
</div>
<% content_for :sidebar, partial("base/sidebar") %>
